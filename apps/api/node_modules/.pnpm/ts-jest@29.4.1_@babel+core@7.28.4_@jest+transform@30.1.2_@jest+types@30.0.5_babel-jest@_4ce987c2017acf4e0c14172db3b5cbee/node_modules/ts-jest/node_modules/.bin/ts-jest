#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/selim/Documents/dropshipping-monorepo/apps/api/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.4_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_4ce987c2017acf4e0c14172db3b5cbee/node_modules/ts-jest/node_modules:/Users/selim/Documents/dropshipping-monorepo/apps/api/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.4_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_4ce987c2017acf4e0c14172db3b5cbee/node_modules:/Users/selim/Documents/dropshipping-monorepo/apps/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/selim/Documents/dropshipping-monorepo/apps/api/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.4_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_4ce987c2017acf4e0c14172db3b5cbee/node_modules/ts-jest/node_modules:/Users/selim/Documents/dropshipping-monorepo/apps/api/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.4_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_4ce987c2017acf4e0c14172db3b5cbee/node_modules:/Users/selim/Documents/dropshipping-monorepo/apps/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
